{
  "name": "@devvit/web-view-scripts",
  "version": "0.12.0",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://developers.reddit.com/"
  },
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "files": [
    "**"
  ],
  "scripts": {
    "build": "tsc && yarn build:devvit",
    "build:context": "yarn build:min src/init-context.ts --outfile=dist/init-context.min.js",
    "build:devvit": "yarn build:min src/devvit.v1.ts --outfile=dist/scripts/devvit.v1.min.js",
    "build:min": "esbuild --bundle --format=esm --minify --sourcemap=linked --target=es2020",
    "clean": "rm -rf .turbo coverage dist",
    "clobber": "yarn clean && rm -rf node_modules",
    "dev": "tsc -w",
    "dev:wvs": "tools/wvs",
    "lint": "redlint .",
    "lint:fix": "yarn lint --fix",
    "prepublishOnly": "publish-package-json",
    "test": "yarn test:unit && yarn test:types && yarn lint",
    "test:size": "yarn build:context && yarn build:devvit && filesize",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:unit-with-coverage": "vitest run --coverage"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@devvit/protos": "0.12.0",
    "@devvit/shared": "0.12.0",
    "@devvit/shared-types": "0.12.0",
    "jwt-decode": "4.0.0"
  },
  "devDependencies": {
    "@ampproject/filesize": "4.3.0",
    "@devvit/repo-tools": "0.12.0",
    "@devvit/tsconfig": "0.12.0",
    "esbuild": "0.23.0",
    "eslint": "9.11.1",
    "typescript": "5.8.3",
    "vitest": "1.6.1"
  },
  "filesize": {
    "dist/init-context.min.js": {
      "gzip": "12 KB",
      "none": "44 KB"
    },
    "dist/scripts/devvit.v1.min.js": {
      "gzip": "0.6 KB",
      "none": "2.0 KB"
    }
  },
  "source": "./src/index.ts",
  "gitHead": "3731eff756778353fa38302d4e5c03fa5f97652b"
}
